\ProvidesPackage{beamerthemeHTWDD}[2017/02/15 HTWDD beamer style]

\mode<presentation>

\usefonttheme{default}
\usecolortheme{HTWDD}
\useoutertheme{HTWDD}
\useinnertheme{HTWDD}

% use square item and enumeration symbols
\setbeamertemplate{items}[square]
\setbeamertemplate{sections/subsections in toc}[square]

% deactivate navigation symbolds (those on the bottom right of frames)
\setbeamertemplate{navigation symbols}{}

\mode
<all>
